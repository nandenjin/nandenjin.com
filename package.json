{
  "name": "@nandenjin/portfolio-nuxt",
  "version": "2.4.0",
  "description": "https://www.nandenjin.com/",
  "main": "index.js",
  "scripts": {
    "start": "nuxt-ts start",
    "dev": "nuxt-ts dev",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "deploy": "firebase deploy",
    "content": "run-s clean content:*",
    "content:fetch": "git clone -b master --depth 1 https://github.com/nandenjin/portfolio.git content",
    "lint": "eslint --ext ts,js,vue .",
    "lintfix": "eslint --ext ts,js,vue . --fix",
    "format": "prettier --write \"./**/*.{js,ts,json,vue,md,yml}\"",
    "clean": "rimraf content"
  },
  "repository": {
    "type": "git",
    "url": "git+https://nandenjin@github.com/nandenjin/portfolio-nuxt.git"
  },
  "author": "Kazumi Inada <hello@nandenjin.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "@nuxt/content": "^1.13.1",
    "@nuxt/types": "^2.14.12",
    "@nuxt/typescript-runtime": "^2.0.2",
    "@nuxtjs/sitemap": "^2.4.0",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.14.12"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^2.0.4",
    "@nuxtjs/eslint-config-typescript": "^2.0.0",
    "@nuxtjs/google-analytics": "^2.3.0",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/sharp": "^0.27.1",
    "consola": "^2.15.3",
    "es6-promise-pool": "^2.5.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-nuxt": "^2.0.0",
    "firebase-tools": "^9.2.2",
    "husky": ">=5",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
    "nuxt-property-decorator": "^2.9.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.6",
    "sass-loader": "^10.1.1",
    "sharp": "^0.27.1",
    "simple-git": "^2.35.2",
    "vue-property-decorator": "^9.1.2"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint --cache --fix",
    "*.{ts,js,vue,md,yml,json}": "prettier --write"
  }
