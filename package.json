{
  "name": "@nandenjin/portfolio-nuxt",
  "version": "1.1.5",
  "description": "https://www.nandenjin.com/",
  "author": "Kazumi Inada <hello@nandenjin.com>",
  "license": "UNLICENSED",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "generate": "yarn workspace @nandenjin/portfolio-nuxt-front generate",
    "deploy": "firebase deploy",
    "content": "run-s clean content:*",
    "content:fetch": "git clone -b master --depth 1 https://github.com/nandenjin/portfolio.git tmp/contents/markdown",
    "content:convert": "md-site-conv tmp/contents/markdown -o tmp/contents/json --index",
    "lint": "run-p lint:*",
    "lint:front": "yarn workspace @nandenjin/portfolio-nuxt-front lint",
    "lint:contents-module": "yarn workspace @nandenjin/portfolio-nuxt-contents-module lint",
    "clean": "rimraf tmp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://nandenjin@github.com/nandenjin/portfolio-nuxt.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@nandenjin/md-site-conv": "https://github.com/nandenjin/md-site-conv.git#master",
    "firebase-tools": "^8.10.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "simple-git": "^2.21.0"
  },
  "private": true
}
